APP_DEBUG=true# 聊天服务 配置CHAT_RUN_REGISTER=trueCHAT_RUN_WORKER=trueCHAT_RUN_GATEWAY=trueCHAT_PID_FILE=chat.pidCHAT_REGISTER_IP=127.0.0.1CHAT_REGISTER_PORT=1234CHAT_WORKER_NAME=ChatBusinessWorkerCHAT_WORKER_COUNT=4CHAT_WORKER_EVENT_HANDLER=\app\event\ChatEventCHAT_GATEWAY_PORT=2200CHAT_GATEWAY_START_PORT=2201CHAT_GATEWAY_NAME=ChatGatewayCHAT_GATEWAY_COUNT=2CHAT_GATEWAY_LAN_IP=127.0.0.1CHAT_GATEWAY_PING_DATA=1CHAT_GATEWAY_PING_INTERVAL=30CHAT_GATEWAY_PING_NOT_RESPONSE_LIMIT=1[APP]DEFAULT_TIMEZONE=Asia/ShanghaiHOST=https://wwww.bearadmin.comAPP_KEY=[DATABASE]TYPE=mysqlHOSTNAME=127.0.0.1DATABASE=bear_adminUSERNAME=rootPASSWORD=rootHOSTPORT=3306CHARSET=utf8mb4DEBUG=true[CACHE]DRIVER=file[REDIS]REDIS_HOST=127.0.0.1REDIS_PORT=6379REDIS_PASSWORD=REDIS_SELECT=0[LANG]default_lang=zh-cn[FILESYSTEM]driver=public[API]HTTP_CODE_SYNC=trueALLOW_CROSS_DOMAIN=trueJWT_KEY=JWT_EXP=3600ENABLE_REFRESH_TOKEN=trueREFRESH_TOKEN_EXP=1296000REUSE_CHECK=trueJWT_ISS=sJWT_AUD=aTOKEN_POSITION=headerTOKEN_FIELD=token